{
	"name": "__MSG_extName__",
	"version": "0.9.7.1",
	"manifest_version": 3,
	"description": "__MSG_extDesc__",
	"icons": {
		"128": "icon.png"
	},
	"action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"bookmarks",
		"tabs",
		"favicon"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"minimum_chrome_version": "104",
	"default_locale": "en",
	"omnibox": {
		"keyword" : "*"
	},
	"web_accessible_resources": [
		{
			"resources": ["_favicon/*"],
			"matches": ["<all_urls>"]
		}
	]
}